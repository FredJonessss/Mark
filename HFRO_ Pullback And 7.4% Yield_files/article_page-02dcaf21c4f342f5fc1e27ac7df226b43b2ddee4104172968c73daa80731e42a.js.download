      $attachedToElement = opt.$hoverElement;
      config = {object: opt.object, onRemoveSuccess: opt.onRemoveSuccess};
      config.rtaChecked = opt.rtaChecked === undefined ? true : !!opt.rtaChecked;
      config.cssClasses = opt.cssClasses || [];
