(function(w, SA, $){

  'use strict';

  var scope = SA.UsingNamespace('SA.Models.Authentication'), conf = SA.pageConfig || {};

  scope.staticLoginLink = SA.App.host + '/account/login';

  var showGoogleLogin = function(linkUrl, width, height) {
