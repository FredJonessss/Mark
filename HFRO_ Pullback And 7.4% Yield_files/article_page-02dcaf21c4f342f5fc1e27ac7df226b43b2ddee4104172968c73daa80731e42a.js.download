    }
    cache.set('roadblock_site_type', variationName, getExpiration(DAYS_EXPIRED));
    setRbVariation(variationName);
  }

  function initAbTestRoadblock() {
    if (rbCache) {
      setRbVariation(rbCache);
      SA.Logger.Mone.event('ab_tests', '', '', {
