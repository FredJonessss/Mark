    if (newSignUpFromRb) {
      if (SA.Data.User.loggedIn()) {
        registerRoadblockAlerts();
      }
      SA.Logger.Mone.event('funnel_complete_popup', newSignUpFromRb , 'gplus_signup');
      cp.del('gplus_new_sign_up_from_roadblock');
      cp.del('gplus_roadblock_type');
