    }

    function exceedDayCap() {
      if (!impressionsLimited) {
        return false;
      }

      var nowTs = Date.now(),
        lastViewTs,
        dayTSDiff = 24 * 60 * 60 * 1000,
        viewsArr = getViewsArray();

      if (!viewsArr.length) {
