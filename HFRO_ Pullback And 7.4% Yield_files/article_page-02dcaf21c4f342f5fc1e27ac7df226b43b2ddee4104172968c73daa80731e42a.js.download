      articleEmailTheme = scope.getArticleEmailTheme(opt.pageType);
      correctThemeIds();
    }
    showRbPopup();
    setCookies();
    centerModal();
    $(window).on('resize.roadblock', centerModal);
  };

  scope.showing = function() {
