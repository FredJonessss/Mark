    if(autoClose) {
      clearTimeout(autoClose);
    }
    autoClose = setTimeout(closeFunc, CLOSE_AFTER);
  };

  var setBreakingNewsPresent = function() {
    $body.addClass("breaking-news-present");
  };

  scope.init();

